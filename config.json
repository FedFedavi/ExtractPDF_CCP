{
  "enable_console_log": true,
  "rules": [
    {
      "name": "Вид постановления",
      "regex": "(?i)Постановление\\s+о\\s+.*?производства",
      "join_lines": true,
      "max_lines": 0
    },
    {
      "name": "Дата постановления",
      "regex": "\\d{2}\\.\\d{2}\\.\\d{4}",
      "join_lines": true,
      "max_lines": 0
    },
    {
      "name": "Наименование отдела ФССП",
      "regex": "(?:ОСП|СОСП|ФССП|управление|отделение)[^,\n]+(?:\n[^,\n]+)?(?=\\s*ул\\.|\\n{2,}|$)",
      "join_lines": true,
      "max_lines": 4
    },
    {
      "name": "Наименование должника",
      "regex": "в отношении должника[^:]*?:\\s*(?:юридическое лицо|физическое лицо)\\):?\\s*((?:[^,\\n]+(?:\\n[^,\\n]+)?)?)",
      "join_lines": true,
      "max_lines": 2
    },
    {
      "name": "Номер постановления",
      "regex": "№\\s(\\d{5,}/\\d{2}/\\d{5,}-ИП)",
      "join_lines": true,
      "max_lines": 0
    }
  ]
}
